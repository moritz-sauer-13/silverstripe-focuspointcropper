{
	"name": "restruct/silverstripe-focuspointcropper",
	"description": "Even smarter automatic image cropping for SilverStripe",
	"type": "silverstripe-vendormodule",
	"keywords": ["silverstripe", "crop", "cropping", "focus", "focuspoint", "image"],
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "Restruct",
		"homepage": "http://restruct.nl"
	}],
	"require": {
		"jonom/focuspoint": "^3.0",
        "restruct/silverstripe-simpler": "^0.1"
	},
    "replace": {
        "micschk/silverstripe-focuspointcropper": "*"
    },
    "autoload": {
        "psr-4": {
            "Restruct\\SilverStripe\\FocusPointCropper\\": "src/"
        }
    },
	"extra": {
        "expose": [
            "client"
        ]
    }
}
